{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks"
  },
  "deploy": {
    "startCommand": "python start.py",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300
  },
  "variables": {
    "NIXPACKS_PYTHON_VERSION": "3.11",
    "PYTHON_VERSION": "3.11"
  }
}
